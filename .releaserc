{
    "branches": [
        "main",
        {
            "name": "nightly",
            "channel": "nightly",
            "prerelease": "nightly"
        },
        {
            "name": "rc",
            "channel": "rc",
            "prerelease": "rc"
        },
        {
            "name": "ci",
            "channel": "ci",
            "prerelease": "test-do-not-use"
        }
    ],
    "repositoryUrl": "https://github.com/ArkanisCorporation/ArkanisOverlay",
    "tagFormat": "v${version}",
    "debug": false,
    "plugins": [
        "@semantic-release/commit-analyzer",
        [
            "@semantic-release/exec",
            {
                "verifyReleaseCmd": "VERSION=${nextRelease.version} VERSION_TAG=${nextRelease.gitTag} ./scripts/release-10-verify.sh",
                "prepareCmd": "VERSION=${nextRelease.version} VERSION_TAG=${nextRelease.gitTag} ./scripts/release-20-prepare.sh",
                "publishCmd": "VERSION=${nextRelease.version} VERSION_TAG=${nextRelease.gitTag} ./scripts/release-30-publish.sh"
            }
        ],
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md"
            }
        ],
        [
            "@semantic-release/github",
            {
                "assets": [
                    { "path": "ArkanisOverlay.zip" },
                    { "path": "CHANGELOG.md" }
                ]
            }
        ]
    ]
}
